<h1>Books</h1>
<p><strong>Title</strong></p>
<p><strong>Body</strong></p>
<% @indexes.each do |book| %>
  <div class="index">
  <span><%= book.title %></span>

  <span><%= book.body %></span>
  </div>
<% end %>

<h2>New book</h2>
<%= form_with model: @books,url: '/books',method: :post do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<% end %>